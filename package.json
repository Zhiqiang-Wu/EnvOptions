        "loadsh": "^0.0.4",
        "lowdb": "^3.0.0",
        "react": "17.x",
        "react-dom": "17.x",
        "recompose": "^0.30.0",
        "redux-actions": "^2.6.5",
        "regedit": "^5.0.0",
        "reselect": "^4.0.0",
        "sqlite3": "^5.0.2",
        "umi": "^3.5.18",
        "winston": "^3.3.3",
        "winston-daily-rotate-file": "^4.5.5"
    },
    "devDependencies": {
        "@types/ffi-napi": "^4.0.4",
        "@types/lowdb": "^1.0.11",
        "@types/node": "^14.6.2",
