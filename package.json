        "@types/recompose": "^0.30.8",
        "@types/redux-actions": "^2.6.2",
        "@types/reselect": "^2.2.0",
        "@types/sqlite3": "^3.1.7",
        "@types/winston": "^2.4.4",
        "@umijs/plugin-sass": "^1.1.1",
        "@umijs/preset-react": "1.x",
        "@umijs/test": "^3.5.18",
        "electron": "^14.0.0",
        "electron-builder": "^22.11.7",
        "lint-staged": "^10.0.7",
        "node-sass": "4.14.1",
        "prettier": "^2.2.0",
        "sass-loader": "^12.1.0",
        "typescript": "^4.1.2",
        "umi-plugin-electron-builder": "^2.1.4",
        "yorkie": "^2.0.0"
